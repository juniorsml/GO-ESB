#%RAML 0.8

title: Arizona GO Project API
baseUri: http://localhost:8081/api/v1
version: 1.0
documentation:
  - title: Arizona GO Project
    content: |
      Arizona Advertising Campaign API Manager
 
traits:
 - permissionRequired:
     displayName: permissionRequired
     queryParameters:
      agencyId:
        type: String
        required: false
        # not required, because when profile='arizona', 
        # the admin may be interested in looking at campaigns across all agencies
      profile:
        description: the profile used to log in to the Go portal
        # could be replaced by a profileId as an integer
        type: string
        required: true
    
 - paged:
    displayName: paged
    queryParameters:
     offset:
      example: ?offset=10
      displayName: offset
      description: Set the init register to return
      type: string
     limit:
      example: ?limit=5
      displayName: limit
      description: Max number os registers to return
      type: string
           

                         
/campaigns:
    displayName: Campaigns
    description: Retrieves a list of campaigns, filtered (by agencya and profile) and with pagination           
    get:
      is: [ paged , permissionRequired ]
      responses:
        200:
          description: |
            Campaigns retrieved successfully
          body:
            application/json:
              schema: !include schemas/go-project-schema.json
              example: !include examples/campaigns-get-example.json                   
    post:
      is: [ permissionRequired ]
      description: Create a New Campaign
      body:
        application/json:
          schema: !include schemas/campaign-post-canonical.json
          example: !include examples/campaign-post-example.json
      responses:
        200:
          description: Success Campaign Created
          body:
            application/json:
              example: examples/campaign-get-example.json                   
                                                    
    
    /{campaignId}:
      displayName: Get a single Campaign by Id
      description: A specific campaign, a member of the campaigns collection
      get:
        is: [ permissionRequired ]  
        responses:
          200:
            description: |
              Campaign retrieved successfully
            body:
              application/json:
                schema: !include schemas/go-project-schema.json
                example: !include examples/campaign-get-example.json
                
    
      put:
        description: Update Campaign
        body:
          application/json:
            schema: !include schemas/campaign-put-canonical.json
            example: !include examples/campaign-put-example.json
        responses:
          200:
            description: |
              Updated Campaign
            body:
              application/json:
                example: examples/campaign-put-example.json
                
      delete:
        description: delete Campaign
        body:
          application/json:
        responses:
          200:
            description: |
              Updated Campaign
            body:
              application/json:
                example: examples/campaign-get-example.json
    
      /history:
        post:
          description:  Create a History Campaign  
          body:
            application/json:
              schema: !include schemas/campaign-history-post-canonical.json
              example: !include examples/campaign-history-post-example.json
          responses:
            200:
              description: Create a history from one action in campaign
              body:
                application/json:
                  example: examples/campaign-history-post-example.json
                       
        get:
          is : [permissionRequired , paged]      
          responses:
            200:
              description: |
                Campaigns retrieved successfully
              body:
                application/json:
                  schema: !include schemas/go-project-schema.json
                  example: !include examples/campaign-history-get-example.json        
      /status:
        put:
          description: Update Status Campaign
          body:
            application/json:
              schema: !include schemas/campaign-status-put-canonical.json
              example: !include examples/campaign-status-put-example.json
          responses:
            200:
              description: Campaign status changed
              body:
                application/json:
                  example: examples/campaign-status-put-example.json
            
                  
      /files:
        post:
          description: Upload file content for a campaign                    
          body:
            binary/octet-stream:
            multipart/form-data:
              formParameters:
                file:
                  description: The file to be uploaded
                  required: true
                  type: file
                  #example: !include examples/baseFileSample.pdf
                filename:
                 description: Name of the file uploaded
                 type: string
                 example: arquivo.jpg
          responses: 
           200: 
            body: 
             application/json: 
              example: !include  examples/campaign-file-get-example.json       
        
        get:
           is : [ permissionRequired , paged]          
           responses:
              200:
                description: |
                  Campaigns retrieved successfully
                body:
                  application/json:
                    schema: !include schemas/go-project-schema.json
                    example: !include examples/campaign-files-get-example.json 
               
                     
        /thumbs:
          get:
            is : [ permissionRequired ]  
            body:
              binary/octet-stream:
              multipart/form-data:
                formParameters:
                  file:
                    description: The file to be uploaded
                    required: true
                    type: file
                    #example: !include examples/baseFileSample.pdf
            responses: 
             200: 
              body: 
               application/json: 
                example: !include  examples/campaign-file-get-example.json     
               
                 
                 
                   
                   
         
                   
        /{fileId}:
          get:
            description: Get one file information
            is : [ permissionRequired ]            
            responses:
              200:
                description: |
                  Campaign file retrieved successfully
                body:
                  application/json:
                    schema: !include schemas/go-project-schema.json
                    example: !include examples/campaign-file-get-example.json
                 
          post:
            description: Upload a new file to a campaign
            body:
              application/json:
                example: !include examples/campaign-file-post-example.json
            responses:
              200:
                description: |
                  Request Acknowledged
                body:
                  application/json:
                    example: !include examples/campaign-file-post-example.json       
          
          delete:
            description: Remove a file from a campaign
            body:
              application/json:
            responses:
              200:
                description: Remove a file from campaign
                body:
                  application/json:
                    example: |
                      {
                        "fileid":"1",
                        "message" : "removed" 
                       }       
          
          put:
            description: update file to a campaign
            body:
              application/json:
                example: !include examples/campaign-file-put-example.json
            responses:
              200:
                description: update a file information 
                body:
                  application/json:
                    example: examples/campaign-file-get-example.json       
                 
          /thumbs:
            get:
              description: Get one file information
              queryParameters:
              is : [ permissionRequired , paged]            
              responses:
                200:
                  
                     
                             
            post:
              description: |
                 create a thumb to the file 
                 Use the "binary/octet-stream" content type to specify the content from any consumer (excepting web-browsers).
                 Use the "multipart-form/data" content type to upload a file which content will become the file-content
              body:  
                binary/octet-stream:
                multipart/form-data:
                    formParameters:
                      file:
                        description: The file to be uploaded
                        required: true
                        type: file
                        #example: !include examples/baseFileSample.pdf
              responses: 
                 200: 
                  body: 
                   application/json: 
                    example: !include  examples/campaign-file-get-example.json   
            delete:
              description: Upload a new file to a campaign
              body:
                application/json:
              responses:
                200:
                  description: |
                    Remove thumb success
                  body:
                    application/json:
                      example: |
                        {
                          "fileId": "1"
                          "status": "Removed"
                        }       
            
            put:
              description: |
                 replace a thumb to the file 
                 Use the "binary/octet-stream" content type to specify the content from any consumer (excepting web-browsers).
                 Use the "multipart-form/data" content type to upload a file which content will become the file-content
              body:
                binary/octet-stream:
                multipart/form-data:
                  formParameters:
                    file:
                      description: The file to be uploaded
                      required: true
                      type: file     
                              
                
          /download:
            get:
              description: Get one file information
              is : [ permissionRequired ]  
              queryParameters:
                fileType:
                  description: normalized ou ISO
              responses:
                200:
                  body:
                    binary/octet-stream:
          
      /notifications:
