/user:
  displayName: User
  description: Retrieves a list of users, filtered (by profile) and with pagination 
  # method only available to 'Arizona' profile          
  get:
    is : [permissionRequired , paged]          
    responses:
      200:
        description: |
          Users retrieved successfully
        body:
          application/json:
            example: !include examples/users-get-example.json  
                   
  post:
    description: New User
    body:
      application/json:
        schema: !include schemas/user-post-canonical.json
        example: !include examples/user-post-example.json
    responses:
      200:
